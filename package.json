{
  "name": "lan_control",
  "version": "1.0.0",
  "description": "a remote control in LAN, support keyboard, mouse, clipboard capture and replay",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "me.xk.lan_control",
    "productName": "lan_control",
    "copyright": "Copyright Â© 2020 ${author}",
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator",
      "icon": "assets/app.ico"
    },
    "linux": {
      "target": "deb",
      "icon": "assets/",
      "category": "Utility",
      "maintainer": "${author}"
    }
  },
  "keywords": [],
  "author": "xkfront",
  "email": "xkfront@outlook.com",
  "url": "https://xiekun1992.github.io/",
  "license": "ISC",
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "@xiekun1992/node-addon-clipboard-auto": "*",
    "@xiekun1992/node-addon-keyboard-auto": "*",
    "@xiekun1992/node-addon-transparent-window": "*",
    "auto-launch": "^5.0.5",
    "systeminformation": "^4.27.11"
  }
}
